  "polling_state_rpc"           {>= "v0.17" & < "v0.18"}
  "ppx_css"                     {>= "v0.17" & < "v0.18"}
  "ppx_diff"                    {>= "v0.17" & < "v0.18"}
  "ppx_here"                    {>= "v0.17" & < "v0.18"}
  "ppx_jane"                    {>= "v0.17" & < "v0.18"}
  "ppx_let"                     {>= "v0.17" & < "v0.18"}
  "ppx_pattern_bind"            {>= "v0.17" & < "v0.18"}
