  "base"                     {= "v0.18~preview.130.76+222"}
  "bonsai"                   {= "v0.18~preview.130.76+222"}
  "bonsai_concrete"          {= "v0.18~preview.130.76+222"}
  "core"                     {= "v0.18~preview.130.76+222"}
  "core_unix"                {= "v0.18~preview.130.76+222"}
  "expect_test_helpers_core" {= "v0.18~preview.130.76+222"}
  "expectable"               {= "v0.18~preview.130.76+222"}
  "incr_map"                 {= "v0.18~preview.130.76+222"}
  "incremental"              {= "v0.18~preview.130.76+222"}
  "ocaml-embed-file"         {= "v0.18~preview.130.76+222"}
  "patdiff"                  {= "v0.18~preview.130.76+222"}
  "ppx_expect"               {= "v0.18~preview.130.76+222"}
  "ppx_jane"                 {= "v0.18~preview.130.76+222"}
  "ppx_pattern_bind"         {= "v0.18~preview.130.76+222"}
  "ppx_quick_test"           {= "v0.18~preview.130.76+222"}
  "virtual_dom"              {= "v0.18~preview.130.76+222"}
  "dune"                     {>= "3.17.0"}
  "re"                       {= "1.14.0+ox"}
]
available: arch != "arm32" & arch != "x86_32"
synopsis: "A library for testing Bonsai state machines."
description: """
Bonsai is a library for building composable state machines. This libary
