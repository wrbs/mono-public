opam-version: "2.0"
maintainer: "Jane Street developers"
authors: ["Jane Street Group, LLC"]
homepage: "https://github.com/janestreet/core_unix"
bug-reports: "https://github.com/janestreet/core_unix/issues"
dev-repo: "git+https://github.com/janestreet/core_unix.git"
doc:
  "https://ocaml.janestreet.com/ocaml-core/latest/doc/core_unix/index.html"
license: "MIT"
build: [
  ["dune" "build" "-p" name "-j" jobs]
]
depends: [
  "ocaml"                    {>= "5.1.0"}
  "base"                     {= "v0.18~preview.130.76+222"}
  "core"                     {= "v0.18~preview.130.76+222"}
  "core_kernel"              {= "v0.18~preview.130.76+222"}
  "expect_test_helpers_core" {= "v0.18~preview.130.76+222"}
  "jane-street-headers"      {= "v0.18~preview.130.76+222"}
  "jst-config"               {= "v0.18~preview.130.76+222"}
  "ppx_diff"                 {= "v0.18~preview.130.76+222"}
  "ppx_jane"                 {= "v0.18~preview.130.76+222"}
  "ppx_optcomp"              {= "v0.18~preview.130.76+222"}
  "ppx_stable_witness"       {= "v0.18~preview.130.76+222"}
  "sexplib"                  {= "v0.18~preview.130.76+222"}
  "uopt"                     {= "v0.18~preview.130.76+222"}
  "base-threads"
  "dune"                     {>= "3.17.0"}
  "spawn"                    {= "v0.15.1+ox"}
]
available: arch != "arm32" & arch != "x86_32"
synopsis: "Unix-specific portions of Core"
description: """
Unix-specific extensions to some of the modules defined in [core] and [core_kernel].
"""
depexts: ["linux-headers"] {os-family = "alpine"}
url {
  src:
    "https://github.com/janestreet/core_unix/archive/63390a3f75376156e77ed49d6cc07ca48a90dd53.tar.gz"
  checksum:
    "sha256=7d3aa4f8debf4cd7d3a6d5f57f42ed173eb36aac16a55a9b7b1496a9c683fc10"
}
