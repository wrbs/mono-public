  "ocaml-embed-file" {= "v0.18~preview.130.76+222"}
  "ppx_jane"         {= "v0.18~preview.130.76+222"}
  "ppx_jsonaf_conv"  {= "v0.18~preview.130.76+222"}
  "dune"             {>= "3.17.0"}
  "httpaf"           {>= "0.7.1"}
  "jingoo"           {>= "1.4.4" & < "1.5.0"}
]
available: arch != "arm32" & arch != "x86_32"
synopsis: "An OpenAPI 3 to OCaml client generator."
description: """
An OpenAPI 3 to OCaml client generator.
"""
