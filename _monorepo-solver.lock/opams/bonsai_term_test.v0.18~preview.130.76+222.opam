opam-version: "2.0"
maintainer: "Jane Street developers"
authors: ["Jane Street Group, LLC"]
homepage: "https://github.com/janestreet/bonsai_term_test"
bug-reports: "https://github.com/janestreet/bonsai_term_test/issues"
dev-repo: "git+https://github.com/janestreet/bonsai_term_test.git"
doc:
  "https://ocaml.janestreet.com/ocaml-core/latest/doc/bonsai_term_test/index.html"
license: "MIT"
build: [
  ["dune" "build" "-p" name "-j" jobs]
]
depends: [
  "ocaml"                    {>= "5.1.0"}
  "async"                    {= "v0.18~preview.130.76+222"}
  "bonsai"                   {= "v0.18~preview.130.76+222"}
  "bonsai_term"              {= "v0.18~preview.130.76+222"}
  "bonsai_term_components"   {= "v0.18~preview.130.76+222"}
  "bonsai_test"              {= "v0.18~preview.130.76+222"}
  "core"                     {= "v0.18~preview.130.76+222"}
  "expect_test_helpers_core" {= "v0.18~preview.130.76+222"}
  "expectable"               {= "v0.18~preview.130.76+222"}
  "notty_async"              {= "v0.18~preview.130.76+222"}
  "patdiff"                  {= "v0.18~preview.130.76+222"}
  "ppx_jane"                 {= "v0.18~preview.130.76+222"}
  "ppx_quick_test"           {= "v0.18~preview.130.76+222"}
  "ppx_with"                 {= "v0.18~preview.130.76+222"}
  "dune"                     {>= "3.17.0"}
  "notty-community"          {= "0.2.4+ox"}
  "re"                       {= "1.14.0+ox"}
]
available: arch != "arm32" & arch != "x86_32"
synopsis: "A library for building dynamic terminal apps, using bonsai"
description: """
Bonsai_term is a library for building reusable TUI componetns and apps inside an
    Incremental-style UI framework such as Bonsai_web, Incr_dom or React.
"""
url {
  src:
    "https://github.com/janestreet/bonsai_term_test/archive/547f7d54f198b00bba5a0434dee57cccd1f4d8d4.tar.gz"
  checksum:
    "sha256=697bdf7592d5d4acd461df763a57dc3ea22dc208bec647c45b505dcc9b0e632e"
}
