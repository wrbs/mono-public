  src:
    "https://github.com/janestreet/toplayer/archive/02e927635e5c7c9a125e33d7f7eb8ec7857cef70.tar.gz"
  checksum:
    "sha256=46d30df5241f14a2a6ae34a4cc77c266488beb808e5746cae763925a912baa17"
}
