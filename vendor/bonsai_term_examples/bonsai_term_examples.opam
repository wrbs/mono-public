opam-version: "2.0"
maintainer: "Jane Street developers"
authors: ["Jane Street Group, LLC"]
homepage: "https://github.com/janestreet/bonsai_term_examples"
bug-reports: "https://github.com/janestreet/bonsai_term_examples/issues"
dev-repo: "git+https://github.com/janestreet/bonsai_term_examples.git"
doc: "https://ocaml.janestreet.com/ocaml-core/latest/doc/bonsai_term_examples/index.html"
license: "MIT"
build: [
  ["dune" "build" "-p" name "-j" jobs]
]
depends: [
  "ocaml" {>= "5.1.0"}
  "async"
  "bonsai"
  "bonsai_term"
  "bonsai_term_components"
  "bonsai_term_test"
  "bonsai_test"
  "core"
  "core_unix"
  "ppx_embed_file"
  "ppx_expect"
  "ppx_jane"
  "dune"                   {>= "3.17.0"}
]
available: arch != "arm32" & arch != "x86_32"
synopsis: "Some examples for bonsai_term"
description: "
Bonsai_term is a library for building reusable TUI componetns and apps
   inside an Incremental-style UI framework such as Bonsai_web, Incr_dom or
   React. This library contains some examples that you can reference on how to
   use `bonsai_term`.
"
