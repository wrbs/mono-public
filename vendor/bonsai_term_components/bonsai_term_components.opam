opam-version: "2.0"
maintainer: "Jane Street developers"
authors: ["Jane Street Group, LLC"]
homepage: "https://github.com/janestreet/bonsai_term_components"
bug-reports: "https://github.com/janestreet/bonsai_term_components/issues"
dev-repo: "git+https://github.com/janestreet/bonsai_term_components.git"
doc: "https://ocaml.janestreet.com/ocaml-core/latest/doc/bonsai_term_components/index.html"
license: "MIT"
build: [
  ["dune" "build" "-p" name "-j" jobs]
]
depends: [
  "ocaml" {>= "5.1.0"}
  "bonsai"
  "bonsai_concrete"
  "bonsai_term"
  "core"
  "core_kernel"
  "expectree"
  "ppx_jane"
  "sexplib"
  "vec"
  "dune"            {>= "3.17.0"}
  "notty-community" {= "0.2.4+ox"}
  "react"
]
available: arch != "arm32" & arch != "x86_32"
synopsis: "A set of Terminal UI (TUI) component libraries for bonsai_term."
description: "
bonsai_term_components is an opam package that contains a couple of bonsai_term TUI components.
"
